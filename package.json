{
  "name": "pr-staleness-checker",
  "version": "1.0.0",
  "main": "dist/team.mjs",
  "scripts": {
    "dev": "tsx src/team.ts",
    "dev:team": "tsx src/team.ts",
    "dev:whole": "tsx src/whole.ts",
    "dev:community": "tsx src/community.ts",
    "build": "npm run clean && ENTRY=team vite build && ENTRY=whole vite build && ENTRY=community vite build && npm run fix-fetch",
    "fix-fetch": "sed -i '' 's/fetch\\$1/fetch/g' dist/*.mjs",
    "start:team": "node dist/team.mjs",
    "start:whole": "node dist/whole.mjs",
    "start:community": "node dist/community.mjs",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.31",
    "tsx": "^4.19.2",
    "vite": "^6.3.5"
  },
  "type": "module"
}
